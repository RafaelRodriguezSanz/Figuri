{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "ucu.edu.uy.App",
            "projectName": "Main"
        },
        {
            "type": "java",
            "name": "Launch TestSQLStatement",
            "request": "launch",
            "mainClass": "ucu.edu.uy.TestSQLStatement",
            "projectName": "Main"
        },
        {
            "type": "java",
            "name": "Launch TestSQLStatement",
            "request": "launch",
            "mainClass": "ucu.edu.uy.TestSQLStatement",
            "projectName": "Agent"
        },
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "ucu.edu.uy.App",
            "projectName": "Agent"
        },
        {
            "type": "java",
            "name": "Launch MyAgent",
            "request": "launch",
            "mainClass": "ucu.edu.uy.jade.agents.MyAgent",
            "projectName": "Agent"
        },
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch Platform",
            "request": "launch",
            "mainClass": "jade.Boot",
            "args": [
                "-gui",
                "-port",
                "1111"
            ]
        },
        {
            "type": "java",
            "name": "Launch Agent",
            "request": "launch",
            "mainClass": "jade.Boot",
            "args": [
                "-container",
                "-host",
                "192.168.9.1",
                "-port",
                "1111",
                "-agents",
                "B:ucu.edu.uy.Jade.Agents.AgentTest"
            ]
        },
        {
            "type": "java",
            "name": "Launch RMA",
            "request": "launch",
            "mainClass": "jade.Boot",
            "args": [
                "-container",
                "-host",
                "192.168.9.1",
                "-port",
                "1111",
                "-agents",
                "RMA:jade.tools.rma.rma"
            ]
        },
        {
            "type": "java",
            "name": "Launch JavaFX",
            "request": "launch",
            "preLaunchTask": "executeJavaFX"
        }
    ]
}